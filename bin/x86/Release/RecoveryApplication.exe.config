<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="SQLitePCLRaw.core" publicKeyToken="1488e028ca7ab535" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.1.9.2410" newVersion="2.1.9.2410" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

	<appSettings>
		 <add key="SourceDirectory" value="C:\DefaultSourcePath" />
         <add key="DestinationDirectory" value="C:\DefaultDestinationPath" />
         <add key="GPGKey_Archive" value="DefaultArchiveKey" />
         <add key="GPGKey_Recovery" value="DefaultRecoveryKey" />
         <add key="ArchiveFrequency" value="24" />
		<!-- Frequency in hours -->
	</appSettings>
</configuration>